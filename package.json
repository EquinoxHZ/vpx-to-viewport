{
  "name": "postcss-vpx-to-vw",
  "version": "1.5.0",
  "description": "PostCSS plugin to convert vpx units to vw, maxvpx to max(vw, Npx) for minimum bounds, minvpx to min(vw, Npx) for maximum bounds, and cvpx to clamp(minPx, vw, maxPx) for responsive range bounds in responsive design",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint index.js",
    "lint:fix": "eslint index.js --fix",
    "demo:basic": "node demo/basic-test.js",
    "demo:pseudo": "node demo/pseudo-elements-test.js",
    "demo:ratio": "node demo/ratio-test.js",
    "demo:validate": "node demo/validate.js",
    "demo:examples": "node demo/examples.js",
    "demo:benchmark": "node demo/benchmark.js",
    "demo:log": "node demo/log-demo.js",
    "demo:cvpx": "node demo/cvpx-demo.js",
    "demo:media-query": "node demo/media-query-demo.js",
    "prepublishOnly": "npm run demo:validate && npm run lint && npm test"
  },
  "keywords": [
    "postcss",
    "postcss-plugin",
    "css",
    "vpx-to-viewport",
    "viewport",
    "vpx",
    "vw",
    "responsive",
    "mobile-first"
  ],
  "author": "Felix Zhu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EquinoxHZ/vpx-to-viewport.git"
  },
  "bugs": {
    "url": "https://github.com/EquinoxHZ/vpx-to-viewport/issues"
  },
  "homepage": "https://github.com/EquinoxHZ/vpx-to-viewport#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "peerDependencies": {
    "postcss": "^8.4.0"
  },
  "dependencies": {
    "postcss": "^8.4.31"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "eslint": "^8.0.0"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "README.md",
    "CHANGELOG.md"
  ]
}
